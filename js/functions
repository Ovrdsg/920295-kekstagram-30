function checkLeng(string, length) {
  return string.length <= length;
}

// eslint-disable-next-line no-console
console.log(checkLeng('пример',3));
// eslint-disable-next-line no-console
console.log(checkLeng('пример',7));

function isPalindrome(string) {
  return string === string.split('').reverse().join('');
}
// eslint-disable-next-line no-console
console.log(isPalindrome('потоп'));
// eslint-disable-next-line no-console
console.log(isPalindrome('Кекс'));

const isMeetingInWorkingHours= (startTime, endTime, meetingStart, meetingDuration) => {
  const startMinutes = timeToMinutes(startTime);
  const endMinutes = timeToMinutes(endTime);
  const meetindEnd = meetingStart + meetingDuration;

  if(meetingStart < startMinutes || meetindEnd > endMinutes) {
    return false;
  }
  else {
    return true;
  }
};

const timeToMinutes = (time) => {
  const [hours, minutes] = time.split(':');
  const hoursNumber = parseInt(hours,10);
  const minutesNumber = parseInt(minutes,10);
  const totalMInutes = hoursNumber * 60 + minutesNumber;
  return totalMInutes;
};
const isMeetingOk = isMeetingInWorkingHours('08:00', '12:00', timeToMinutes('09:30'), 120);
console.log(isMeetingOk);
